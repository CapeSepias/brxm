<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0"
         id="crispstudio">

  <display-name>CRISP Developer Studio</display-name>
  <description>CRISP Developer Studio</description>

  <servlet>
    <servlet-name>ResourceServiceBrokerProxyServlet</servlet-name>
    <servlet-class>org.onehippo.cms7.crisp.studio.servlet.ResourceServiceBrokerProxyServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ResourceServiceBrokerProxyServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>

</web-app>
