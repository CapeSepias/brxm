{
  "name": "hippo-cm",
  "private": true,
  "author": "Hippo B.V.",
  "license": "Apache-2.0",
  "description": "Manage channels for Bloomreach Experience",
  "repository": {
    "type": "git",
    "url": "https://code.onehippo.org/cms-community/hippo-addon-channel-manager"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "engines": {
    "node": "8",
    "npm": "6"
  },
  "scripts": {
    "build": "webpack --env.prod",
    "build:debug": "node --inspect-brk node_modules/webpack-cli/bin/cli.js --env.dev",
    "build:dev": "webpack --env.dev",
    "build:dll": "webpack --env.dll",
    "build:profile": "webpack --env.prod --profile --verbose",
    "clean": "node_modules/@bloomreach/frontend-build/bin/clean.js",
    "clean:lint:css": "node_modules/@bloomreach/frontend-build/bin/clean.js .cache/stylelint",
    "clean:lint:js": "node_modules/@bloomreach/frontend-build/bin/clean.js .cache/eslint",
    "dev:all": "concurrently -k \"npm:lint:watch\" \"npm:start\" \"npm:test\"",
    "lint": "concurrently --names \"js,css\" -c \"blue.bold,magenta.bold\" \"npm -s run lint:js\" \"npm -s run lint:css\"",
    "lint:css": "stylelint \"src/**/*.scss\" --custom-formatter=\"node_modules/stylelint-formatter-pretty\"",
    "lint:css:watch": "npm run clean:lint:css && onchange -i \"src/**/*.scss\" -- npm -s run lint:css -- --cache --cache-location \".cache/stylelint\"",
    "lint:fix": "concurrently --names \"js,css\" -c \"blue.bold,magenta.bold\" \"npm -s run lint:js -- --fix\" \"npm -s run lint:css -- --fix\"",
    "lint:fix:watch": "concurrently --names \"js,css\" -c \"blue.bold,magenta.bold\" \"npm -s run lint:js:watch -- --fix\" \"npm -s run lint:css:watch -- --fix\"",
    "lint:js": "eslint src/ --format \"node_modules/eslint-formatter-friendly\"",
    "lint:js:watch": "npm run clean:lint:js && esw src/ --watch --format \"node_modules/eslint-formatter-friendly\" --cache --cache-location \".cache/eslint\"",
    "lint:watch": "concurrently --names \"js,css\" -c \"blue.bold,magenta.bold\" \"npm -s run lint:js:watch\" \"npm -s run lint:css:watch\"",
    "start": "webpack-serve --env.dev",
    "start:debug": "node --inspect-brk node_modules/webpack-serve/cli.js --env.dev",
    "start:dist": "webpack-serve --env.prod",
    "start:friendly": "webpack-serve --env.dev --friendly --log-level silent",
    "test": "karma start --env.test",
    "test:friendly": "karma start --env.test --friendly",
    "test:debug": "karma start --env.test --debug",
    "test:single-run": "karma start --single-run --env.test"
  },
  "dependencies": {
    "@bloomreach/dragula": "3.7.2-br.3",
    "@mdi/font": "2.2.43",
    "@uirouter/angularjs": "1.0.15",
    "@uirouter/sticky-states": "1.5.0",
    "@uirouter/visualizer": "6.0.0",
    "angular": "1.6.9",
    "angular-animate": "1.6.9",
    "angular-aria": "1.6.9",
    "angular-local-storage": "0.7.1",
    "angular-material": "1.1.8",
    "angular-messages": "1.6.9",
    "angular-translate": "2.17.0",
    "angular-translate-loader-static-files": "2.17.0",
    "angular-ui-tree": "2.22.6",
    "emittery": "0.4.1",
    "es6-shim": "0.35.3",
    "jquery": "3.3.1",
    "moment-timezone": "0.5.14",
    "mutation-summary": "0.1.1",
    "ng-device-detector": "4.0.3",
    "ng-focus-if": "1.0.7",
    "open-sans-fontface": "github:FontFaceKit/open-sans#59493162b016dae6ef0d06693ccf3c93c1e96091",
    "penpal": "3.0.3",
    "throttle-debounce": "2.0.1"
  },
  "devDependencies": {
    "@bloomreach/frontend-build": "9.0.4",
    "angular-mocks": "1.6.9"
  }
}
